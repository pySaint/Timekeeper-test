{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Playwright tests",
        "console": "integratedTerminal",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run-script", "vscode-debug", "--", "${fileBasename}"],
        "cwd": "${workspaceFolder}/timezone-app/"
      }
    ]
  }